{
  "name": "fitness-tracker",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.7",
    "@mui/material": "^5.15.7",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
