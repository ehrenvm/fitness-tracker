    "@typescript-eslint/parser": "^5.62.0",
    "ajv": "^8.12.0",
    "dotenv": "^16.5.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "html-webpack-plugin": "^5.6.5",
    "jest-junit": "^16.0.0",
    "react-scripts": "^5.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
